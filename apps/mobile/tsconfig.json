{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "jsx": "react-jsx",
        "moduleResolution": "bundler",
        "strict": true,
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        "src/**/*",
        "app/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}