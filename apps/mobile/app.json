{
    "expo": {
        "name": "StreamSync",
        "slug": "streamsync",
        "version": "0.1.0",
        "orientation": "portrait",
        "userInterfaceStyle": "dark",
        "scheme": "streamsync",
        "splash": {
            "backgroundColor": "#0F0F1A"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.streamsync.app"
        },
        "android": {
            "adaptiveIcon": {
                "backgroundColor": "#0F0F1A"
            },
            "package": "com.streamsync.app"
        },
        "web": {
            "bundler": "metro",
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            "expo-router"
        ]
    }
}